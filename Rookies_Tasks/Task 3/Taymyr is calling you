//package task3;
import java.util.Scanner;
public class Task3 {
    public static void main(String[] args) {
        Scanner in =new Scanner(System.in);
        int n=in.nextInt();
        int m=in.nextInt();
        int z=in.nextInt();
        int count=0;
        for (int i = 1; i <=z; i++) {
            if(i%n==0 && i%m==0)
             count++;   
        }
        System.out.println(count);
    }    
}

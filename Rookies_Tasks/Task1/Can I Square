package can.i.square;
import java.util.Scanner;
public class CanISquare {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int t=in.nextInt();
        while(t>0){
        long n = in.nextLong();
        long sum=0;
            for (int i = 0; i <n; i++) {
              long a= in.nextLong();
              sum+=a;
            }
            long sol=(long)Math.sqrt(sum);
            if(sol*sol == sum)
                System.out.println("YES");
            else 
                System.out.println("NO");
             t--;
        }
    }
    
}
